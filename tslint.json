{
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "no-console": [true, "warn", "error", "group"]
  },
  "rules": {
    "interface-name": false,
    "jsx-no-lambda": true,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "ordered-imports": false,
    "curly": false,
    "member-access": false,
    "variable-name": false,
    "no-console": [true, "warn", "error", "group"]
  }
}
