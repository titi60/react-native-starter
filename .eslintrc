{
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ],
    "import/no-unresolved": 1,
    "react/prop-types": 1
  },
  "globals": {
    "fetch": true
  },
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "settings": {
    "import/resolver": {
      "babel-module": {},
      "react-native": {}
    }
  }
}
