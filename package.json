{
  "name": "rn-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:staged": "jest --findRelatedTests",
    "lint": "eslint ./ --cache --fix --ignore-path .gitignore && tslint --fix --project tsconfig.json",
    "prettify": "yarn prettier './**/*.{ts,tsx}'",
    "sync-locales": "node scripts/locales.js",
    "add-image": "node scripts/images.js",
    "add-component": "hygen componentGenerator new",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf node_modules && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn lint",
      "yarn test:staged",
      "yarn prettify",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-boost": "0.4.4",
    "graphql": "14.5.8",
    "graphql-tag": "2.10.1",
    "react": "16.9.0",
    "react-apollo": "3.1.3",
    "react-native": "0.61.4",
    "react-native-gesture-handler": "1.5.0",
    "react-native-reanimated": "1.4.0",
    "react-native-screens": "^1.0.0-alpha.23",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "1.10.3",
    "styled-components": "4.4.1",
    "styled-system": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-transform-flow-strip-types": "7.6.3",
    "@babel/runtime": "7.7.2",
    "@react-native-community/eslint-config": "0.0.5",
    "@storybook/react-native": "5.2.6",
    "@types/enzyme": "3.10.3",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.0.23",
    "@types/react": "16.9.11",
    "@types/react-native": "0.60.22",
    "@types/react-navigation": "3.4.0",
    "@types/react-test-renderer": "16.9.1",
    "@types/styled-components": "4.4.0",
    "@types/styled-system": "5.1.3",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "3.0.0",
    "csvtojson": "2.0.10",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.5.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-import-resolver-react-native": "0.2.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-module-resolver": "0.15.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "fs": "0.0.1-security",
    "husky": "3.0.9",
    "hygen": "5.0.1",
    "imageoptim": "0.5.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "metro-react-native-babel-preset": "0.57.0",
    "ora": "4.0.2",
    "path": "0.12.7",
    "prettier": "1.19.1",
    "react-dom": "16.11.0",
    "react-test-renderer": "16.9.0",
    "request": "2.88.0",
    "sharp": "0.23.2",
    "ts-jest": "24.1.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.1.0",
    "typescript": "3.7.2",
    "util": "0.12.1"
  }
}
