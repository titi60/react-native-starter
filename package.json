{
  "name": "react_native_starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint ./ --cache --fix --ignore-path .gitignore",
    "sync-locales": "node scripts/locales.js",
    "add-image": "node scripts/images.js",
    "add-component": "hygen componentGenerator new",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf node_modules && yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-boost": "0.3.1",
    "graphql": "14.1.1",
    "graphql-tag": "2.10.1",
    "lottie-react-native": "2.5.11",
    "react": "16.8.3",
    "react-apollo": "2.5.2",
    "react-native": "0.59.1",
    "react-native-config": "0.11.7",
    "react-native-gesture-handler": "1.1.0",
    "react-native-screens": "1.0.0-alpha.22",
    "react-navigation": "3.3.2",
    "styled-components": "4.1.3",
    "styled-system": "4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "2.4.2",
    "csvtojson": "2.0.8",
    "eslint": "5.15.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-babel-module": "5.0.1",
    "eslint-import-resolver-react-native": "0.2.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "fs": "0.0.1-security",
    "husky": "1.3.1",
    "hygen": "4.0.2",
    "imageoptim": "0.5.0",
    "jest": "^24.5.0",
    "lint-staged": "8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "ora": "3.2.0",
    "path": "0.12.7",
    "prettier": "1.16.4",
    "react-test-renderer": "16.8.3",
    "request": "2.88.0",
    "sharp": "0.21.3",
    "util": "0.11.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
