{
  "name": "rn-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:staged": "jest -o --passWithNoTests",
    "tsc": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint ./ --ext .js,.ts,.tsx --cache --fix --ignore-path .gitignore",
    "prettify": "yarn prettier --write './**/*.{ts,tsx}'",
    "sync-locales": "node scripts/locales.js",
    "add-image": "node scripts/images.js",
    "add-component": "hygen componentGenerator new",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf node_modules && yarn",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "ios:patch": "cd ios && fastlane incrementVersionNumber bump_type:patch && cd ..",
    "ios:minor": "cd ios &&  fastlane incrementVersionNumber bump_type:minor && cd..",
    "ios:major": "cd ios &&  fastlane incrementVersionNumber bump_type:major && cd..",
    "ios:beta": "cd ios &&  fastlane beta && cd..",
    "ios:matchPull": "cd ios &&  fastlane matchPull && cd..",
    "ios:addTesters": "cd ios &&  fastlane addTesters && cd..",
    "android:beta": "cd android &&  fastlane beta && cd..",
    "android:deploy": "cd android &&  fastlane deploy && cd.."
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "yarn prettify"
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.6",
    "apollo-boost": "0.4.7",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.1",
    "react": "16.9.0",
    "react-apollo": "3.1.3",
    "react-native": "0.61.4",
    "react-native-gesture-handler": "1.5.3",
    "react-native-reanimated": "1.7.0",
    "react-native-safe-area-context": "0.6.2",
    "react-native-screens": "2.0.0-alpha.29",
    "react-navigation": "4.0.10",
    "react-navigation-stack": "2.0.16",
    "styled-components": "5.0.0",
    "styled-system": "5.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/runtime": "7.8.3",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@react-native-community/eslint-config": "0.0.7",
    "@storybook/react-native": "5.3.9",
    "@storybook/react-native-server": "5.3.9",
    "@types/enzyme": "3.10.4",
    "@types/enzyme-adapter-react-16": "1.0.5",
    "@types/jest": "24.9.1",
    "@types/react": "16.9.11",
    "@types/react-native": "0.60.22",
    "@types/react-navigation": "3.4.0",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "4.4.2",
    "@types/styled-system": "5.1.4",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "chalk": "3.0.0",
    "csvtojson": "2.0.10",
    "detox": "15.1.4",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-import-resolver-react-native": "0.2.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-module-resolver": "0.16.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "fs": "0.0.1-security",
    "husky": "4.2.1",
    "hygen": "5.0.3",
    "imageoptim": "0.5.0",
    "jest": "25.1.0",
    "lint-staged": "10.0.2",
    "metro-react-native-babel-preset": "0.58.0",
    "ora": "4.0.3",
    "path": "0.12.7",
    "prettier": "1.19.1",
    "react-dom": "16.11.0",
    "react-native-dev-menu": "4.0.0",
    "react-test-renderer": "16.12.0",
    "request": "2.88.0",
    "sharp": "0.24.0",
    "ts-jest": "25.0.0",
    "typescript": "3.7.5",
    "util": "0.12.1"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/rnStarter.app",
        "build": "xcodebuild -workspace ios/rnStarter.xcworkspace -scheme rnStarter -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone X"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_3_API_28"
        }
      }
    }
  }
}
